  "license": "MIT",
  "dependencies": {
    "axios": "^0.20.0",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
